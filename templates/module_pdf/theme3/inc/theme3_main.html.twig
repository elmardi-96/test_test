<div style="position:absolute;top:-190px;">
<table class="headFacture ">
	<tbody>
		<tr>
			<td style="width:33,333%;text-align:center;font-size: 10px;">DATE FACTURE</td>
			<td style="width:33,333%;text-align:center;"></td>
			<td style="width:33,333%;text-align:center;font-size: 10px;">N° FACTURE</td>
		</tr>
		<tr></tr>
	</tbody>
</table>
<table class="headFacture">
	<tbody>
		<tr>
			<td style="width:33,333%;text-align:center;font-size: 10px;">{{ cab.datefacture ? cab.datefacture|date("d/m/Y") : "now"|date("d/m/Y")  }}</td>
			<td style="width:33,333%;text-align:center;font-size: 22px;font-family: 'courierbold';">FACTURE</td>
			<td style="width:33,333%;text-align:center;font-size: 10px;">{{ cab.code   }}</td>
		</tr>
		<tr></tr>
	</tbody>
</table>
<br/>
<div class="line line-bottom"></div>

<table class="headFacture second first">
	<tbody>
		<tr>
			<td style="width:25%;font-size: 10px;padding-left:5px;">Nom :
			</td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;">{{ cab.client ? cab.client.societe : null  }}</td>
			<td style="width:25%;font-size: 10px;padding-left:25px;">CP/Ville</td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;">{{ cab.code   }}</td>
		</tr>
		<tr></tr>
	</tbody>
</table>
<table class="headFacture second2">
	<tbody>
		<tr>
			<td style="width:25%;font-size: 10px;padding-left:5px;">Adresse :
			</td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;">{{ cab.client ? cab.client.adresse : null  }}</td>
			<td style="width:25%;font-size: 10px;padding-left:25px;"></td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;"></td>
		</tr>
		<tr></tr>
	</tbody>
</table>

<table class="headFacture second3">
	<tbody>

		<tr>
			<td style="width:25%;font-size: 10px;padding-left:5px;"></td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;"></td>
			<td style="width:25%;font-size: 10px;padding-left:25px;">Tél/Email</td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;">{{ cab.client ? cab.client.tel1 : null  }}
				/
				{{ cab.client ? cab.client.mail1 : null  }}</td>
		</tr>
		<tr></tr>
	</tbody>
</table>
<table class="headFacture second4">
	<tbody>
		<tr>
			<td style="width:25%;font-size: 10px;padding-left:5px;"></td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;height:15px"></td>
			<td style="width:25%;font-size: 10px;padding-left:25px;"></td>
			<td style="width:25%;font-size: 10px;background:white;padding-left:5px;height:15px"></td>
		</tr>
		<tr></tr>
	</tbody>
</table>

<div class="line line-top"></div>
</div>
