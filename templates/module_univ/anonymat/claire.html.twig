{% extends 'base_univ.html.twig' %}


{% block title %}
	 Claire | Controle & impression | UNIV
{% endblock %}


{% block body %}


	<div class="new">

		<div
			class="form-validation-msg" style="display:none">
			{# message flash #}
			{% for label, messages in app.flashes %}
				{% for message in messages %}
					<div data-toastr="{{ label }}" class="toastr-msg">
						{{ message }}
					</div>
				{% endfor %}
			{% endfor %}
			{# form global errors  #}
			<div class="form-errors"></div>
		</div>


		<div id="right-sidebar" class="actions animated"></div>


		<div class="row wrapper  white-bg page-heading  fixed-top-navigation-button">

			<div class="col-lg-4">
				<h2 class="fo">Univ , Controle & impression
				</h2>
				{#{ wo_render_breadcrumbs() }#}
			</div>


			<div class="col-lg-8">
				<div class="title-action">
					<div class="title-action"></div>
				</div>
			</div>
			<div class="border-b"></div>
			<div class="col-lg-12">
				{{ wo_render_breadcrumbs() }}
			</div>
		</div>
		<br/><br/>
		

	</div>

{% endblock %}


{% block javascripts %}
	{{ parent() }}
	{{ encore_entry_script_tags('univ_anonymat') }}
{% endblock %}
