<div class="ibox" >
	<div class="ibox-content  border-padding-none">

		<div class="show">
			<div>
				<div class="col-lg-12 animated fadeInRight border-padding-none">
				<title class="title-text" data-color="#555555">Année</title>
				<div class="card-ribbon card-ribbon-annee evaluation-annee">
						<span>
							<div class="style-position">Année</div>
						</span>
					</div>
					<div class="mail-box-header header-mail-show">

						<h2 class="header-show header-title">
							Année :
							{{promotion.description}}
							<span class="float-right font-normal">
								<a href="{{path("module_univ_evaluation_moyenne_annee_fun",{id:promotion.id})}}" class="href-btn-show btn btn-white btn-sm" data-toggle="tooltip" data-placement="top" title="Print email">
									<i class="fa fa-calculator"></i>
									Calculer moyenne
								</a>							
								
							</span>
							
                    			
						</h2>
						<div class="mail-tools tooltip-demo m-t-md">
							<h5 class="padding-0-show">
								<div class="row">
									<div class="col-md-12 padding-show">
										<h3 class="m-t-none m-b">INFORMATIONS <button class="promotion btn btn-white btn-sm href-btn-show link-style" data-id="{{ promotion.id }}" ><i class="fa fa-info"></i> Informations</button></h3>						

										<div class="hr-line-dashed hr-line-dashed-co"></div>
										<div class="form-group row ph">
											<label class="col-sm-4 required font-weight-bold font-weight-bold">Id</label>
											<div class="col-sm-8">
												<p class="form-control-static">{{promotion.id}}</p>
											</div>
										</div>
										<div class="hr-line-dashed hr-line-dashed-co"></div>

										<div class="form-group row ph">
											<label class="col-sm-4 required font-weight-bold font-weight-bold">Créer par</label>
											<div class="col-sm-8">
												<p class="form-control-static">{{promotion.userCreated ? promotion.userCreated.username}}</p>
											</div>
										</div>
										<div class="hr-line-dashed hr-line-dashed-co"></div>

										<div class="form-group row ph">
											<label class="col-sm-4 required font-weight-bold font-weight-bold">Description
											</label>

											<div class="col-sm-8">
												<p class="form-control-static">{{promotion.description}}</p>
											</div>

										</div>
										<div class="hr-line-dashed hr-line-dashed-co"></div>

										{#
																																																																				<div class="hr-line-dashed hr-line-dashed-co"></div>
																																																																				<div class="form-group row ph">
																																																																					<label class="col-sm-4 required font-weight-bold font-weight-bold">Position actuel</label>
																																																											
																																																																					<div class="col-sm-8">
																																																																						<p class="form-control-static">
																																																																							{% if semestre.positionActuel is defined and semestre.positionActuel is not empty %}
																																																																								{% for key, pos in semestre.positionActuel %}
