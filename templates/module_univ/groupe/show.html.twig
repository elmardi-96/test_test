{% extends 'base_univ.html.twig' %}
{% block title %}Consulter | Groupe & étudiant | UNIV
{% endblock %}
{% block body %}
	<div id="right-sidebar" class="actions animated"></div>

	<div class="row wrapper  white-bg page-heading  fixed-top-navigation-button">
		<div class="col-lg-4">
			<h2 class="fo">Univ , Information division groupe 
			</h2>
		</div>
		<div class="col-lg-8">
			<div
				class="title-action">
				{#% include './module_univ/cours/inc/workflow.html.twig' %#}
				

			</div>
		</div>
		<div class="border-b"></div>
		<div class="col-lg-12">
			{{ wo_render_breadcrumbs() }}
		</div>
		{#{{ render(controller('App\\Controller\\moduleAchat\\CommandefrsController:showbreadcumb',{ 'commande': commande.id})) }}
																																				#}

	</div>
	<br/><br/>
	<div class="row pt-20">
		<div class="col-lg-12">
			<div class="ibox">
				<div class="ibox-content">

					{#{% include './module_achat/facturefrs/inc/workflow_breadcumb.html.twig' %}#}
					{#% include './module_univ/cours/inc/workflow_breadcumb.html.twig' %#}
						<div class="row m-t-sm"> <div class="col-lg-12">
							<div class="panel blank-panel">
								<div class="panel-heading">
									<div class="panel-options">
										{% include './module_univ/groupe/inc/navigation.html.twig' %}
									</div>
								</div>
								<div class="col-md-12">
									<div class="panel-body">

										<div class="tab-content">
											<div class="tab-pane active show" id="tab-1">
												<div class="row">
													<div class="col-md-6 b-r">
														<h3 class="m-t-none m-b">Informations
														</h3>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Id</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivDivisionGroupe.id }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Code</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivDivisionGroupe.code }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Désignation</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivDivisionGroupe.designation }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Abréviation</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivDivisionGroupe.abreviation }}</p>
															</div>
														</div>

													</div>
													<div class="col-md-6">
														{# <h3 class="m-t-none m-b">La liste groupe detail
														</h3>
														<div class="hr-line-dashed"></div>
														<table class="table table-striped fichier-liste-style">
															<thead>
																<tr>

																	<th width="20%">Designation</th>
																	<th width="15%">Abréviation</th>
																	<th width="15%">Crée par</th>
																	<th width="15%">date création</th>

																</tr>
															</thead>
															<tbody>
																{% for detail in UnivDivisionGroupe.divisionGroupeDetails %}
																	<tr>

																		<td>{{detail.designation}}</td>
																		<td>{{detail.abreviation}}</td>
																		<td>{{detail.created|date("m/d/Y")}}</td>

																		<td>{{detail.UserCreated ? detail.UserCreated.nom : '' }}</td>

																	</tr>
																{% endfor %}

															</tbody>

														</table> #}
													</div>
												</div>
											</div>
										</div>
									</div>

								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	{#{ render(controller('App\\Controller\\moduleAchat\\MaFacturefrscabController:showhistorique',{ 'facture': facture.id})) }#}

{% endblock %}


{% block javascripts %}
	{{ parent() }}
	{{ encore_entry_script_tags('univ_groupe') }}
{% endblock %}

