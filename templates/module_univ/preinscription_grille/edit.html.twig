
{% extends 'base_univ.html.twig' %}



{% block body %}


 



{{ form_start(form ,{'attr':{'novalidate': 'novalidate' , 'class':'form'}}) }}
<div class="form-validation-msg" style="display:none">
    {# message flash #}
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div data-toastr="{{ label }}" class="toastr-msg">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
    {# form global errors  #}
    <div class="form-errors">{{form_errors(form)}}</div>
</div>
<div class="row wrapper  white-bg page-heading  fixed-top-navigation-button">


	<div class="col-lg-3">

		<h2 class="fo">Univ, Preinscription Grille</h2>


	</div>


	<div class="col-lg-9">
		<div class="title-action">
			<button class="btn btn-white btn-xs btn-lg my-button" data-style="expand-left" type="submit">
				<i class="fa fa-save"></i>
				{{ 'Modifier'|default('Enregistrer') }}</button>
			{% if  univ_t_preinscription_grille.id > 0 %}
				<!-- <a href="#" class="btn btn-xs btn-danger btn-outline" id="_delete"><i class="fa fa-trash"></i> Supprimer</a>-->
				<div id="test" data-etablissement-id="{{ univ_t_preinscription_grille.id|json_encode }}"></div>

			{% endif  %}
		</div>
	</div>

	<div class="border-b"></div>
	<div class="col-lg-12">

		{{ wo_render_breadcrumbs() }}
	</div>
</div>


<br/><br/>


<div class="row pt-20">
	<div class="col-lg-12 ">
		<div class="ibox">

			<div class="ibox-content">

				<div class="message-success"></div>


				<div class="row m-t-sm">
					<div class="col-lg-12">
						<div class="panel blank-panel">
							<div class="panel-heading">
								<div class="panel-options">
                                    {% include './module_univ/preinscription_grille/inc/navigation.html.twig' %}
									{#% include './module_univ/concours/p_concourscab/ins/navigation.html.twig' %#}
								</div>
							</div>

							<div class="panel-body">
								<div class="col-md-12">
									<div class="tab-content">
										<div class="tab-pane active" id="tab-1">


										
											<div class="row">


												<div class="col-md-6 b-r">
													<div class="form-group row">
														{{ form_label(form.etablissement ,'Etablissement(*) :', {'label_attr':{'class':'col-sm-4'}}) }}
														<div class="col-sm-7">
															{{ form_widget(form.etablissement,{'attr':{'class':'form-control form-control-sm etablissement '}}) }}
															<div class="form-error error-class-tab">
																{{ form_errors(form.etablissement) }}
															</div>
														</div>

													</div>
                                             
													<div class="hr-line-dashed"></div>      

													<div class="form-group row">
														{{ form_label(form.formation ,'Formation (*) :', {'label_attr':{'class':'col-sm-4'}}) }}
														<div class="col-sm-7">
															{{ form_widget(form.formation,{'attr':{'class':'form-control form-control-sm formation'}}) }}

															<div class="form-error error-class-tab">
																{{ form_errors(form.formation) }}
															</div>
														</div>

													</div>
						

													<div class="hr-line-dashed"></div>
													<div class="form-group row">
														{{ form_label(form.moyenne ,'Moyenne (*) :', {'label_attr':{'class':'col-sm-4 col-form-label'}}) }}
														<div class="col-sm-3">
															{{ form_widget(form.moyenne,{'attr':{'class':'form-control form-control-sm','placeholder': 'Moyenne'}}) }}
															
															<div class="form-error error-class-tab">
																{{ form_errors(form.moyenne) }}
															</div>
														</div>

													</div>


					

												</div>

												<div class="col-md-6">

												

												</div>

											</div>


										</div>
									</div>
								</div>

							</div>
						</div>
					</div>


				</div>
			</div>

		</div>
	</div>
	{{form_widget(form._token)}}
	{{ form_end(form,{'render_rest': false}) }}


  

   
{% endblock %}
{% block javascripts %}
     {{ parent() }}
     {{ encore_entry_script_tags('univ_preinscription_grille') }}
{% endblock %}