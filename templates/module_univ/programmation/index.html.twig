{% extends 'base_univ.html.twig' %}
{% block title %}Programmation | Planning & programmation | UNIV
{% endblock %}
{% block stylesheets %}
	{{ parent() }}
	{{ encore_entry_link_tags('univ_planning') }}
{% endblock %}
{% block body %}
	<div class="hide-promotion">
		<div class="modal fade right" id="exampleModalPreview" tabindex="-1" role="dialog" aria-labelledby="exampleModalPreviewLabel" aria-hidden="true" data-backdrop='false'>
			<div class="modal-dialog modal-full-height modal-right modal-sm" role="document">
				<div class="modal-content">
					<div class="modal-header">
						<h3 class="modal-title" id="exampleModalPreviewLabel"></h3>
						<button type="button" class="close" data-dismiss="modal" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<div class="modal-body bo-0">
						<div class="show-promotion"></div>
						<div class="text-center spr" style="display:none;">
							<div class="spinner-border" role="status">
								<span class="sr-only">Loading...</span>
							</div>
						</div>
						<div id="resultats_import"></div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="row wrapper  white-bg page-heading  fixed-top-navigation-button">
		<div class="col-lg-4">
			<h2 class="fo">Univ , Programmation
			</h2>
		</div>
		<div class="col-lg-8">
			<div class="title-action"></div>
		</div>
		<div class="border-b"></div>
		<div class="col-lg-12">
			{{ wo_render_breadcrumbs() }}
		</div>

	</div>
	<br/><br/>


	<div class="row pt-20">
		<div class="col-md-12">
			<div class="panel-container">
				<div class="panel-left">
					<div class="ibox" style="    display: contents;">
						<div class="ibox-content full-height-scroll ">
							<div class="row ">
								<div class="col-md-12">
									<div class="tab-content">

										<h3 class="m-t-none m-b-5">Information formation
										</h3>
										<div class="hr-line-dashed"></div>
										<div id='external-events'>
											<div id="using_json"></div>


										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="splitter"></div>
				<div class="panel-right">
					<div class="ibox">
						<div class="ibox-content full-height-scroll ">
							<div class="row ">

								<div class="col-md-12">
									<h4 class="m-t-none m-b-5">LISTES PROGRAMMATION</h4>
									<div class="hr-line-dashed hr-line-dashed-co"></div>
									<div class="tab-content">


										<div class="col-md-12">

											<table class="t-m table table-striped table-bordered table-hover w-m dataTables-example dataTable" id="grid">
												<thead>
													<tr>
														<th>#</th>
														<th>Code</th>

														<th>Etablissement</th>
														<th>Formation</th>
														<th>Promotion</th>
														<th>Semestre</th>
														<th>Année</th>
														<th>Elément</th>
														<th>Nat.Epv</th>
														<th>Volume.H</th>
													</tr>
												</thead>
												<tfoot>
													<tr>
														<th>#</th>
														<th>Code</th>

														<th>Etablissement</th>
														<th>Formation</th>
														<th>Promotion</th>
														<th>Semestre</th>
														<th>Année</th>
														<th>Elément</th>
														<th>Nat.Epv</th>
														<th>Volume.H</th>
													</tr>
												</tfoot>
											</table>
										</div>
									</div>

								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>


{% endblock %}


{% block javascripts %}
	{{ parent() }}
	{{ encore_entry_script_tags('univ_programmation') }}
{% endblock %}
