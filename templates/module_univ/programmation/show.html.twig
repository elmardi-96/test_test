{% extends 'base_univ.html.twig' %}
{% block title %}Consulter programmation | Planning & programmation | UNIV
{% endblock %}
{% block body %}
	<div id="right-sidebar" class="actions animated"></div>

	<div class="row wrapper  white-bg page-heading  fixed-top-navigation-button">
		<div class="col-lg-4">
			<h2 class="fo">Univ , Information programmation
			</h2>
		</div>
		<div class="col-lg-8">
			<div class="title-action"></div>
		</div>
		<div class="border-b"></div>
		<div class="col-lg-12">
			{{ wo_render_breadcrumbs() }}
		</div>
		{#{{ render(controller('App\\Controller\\moduleAchat\\CommandefrsController:showbreadcumb',{ 'commande': commande.id})) }}
																																																																#}

	</div>
	<br/><br/>
	<div class="row pt-20">
		<div class="col-lg-12">
			<div class="ibox">
				<div class="ibox-content">

					{#{% include './module_achat/facturefrs/inc/workflow_breadcumb.html.twig' %}#}
					{#% include './module_univ/cours/inc/workflow_breadcumb.html.twig' %#}
						<div class="row m-t-sm"> <div class="col-lg-12">
							<div class="panel blank-panel">
								<div class="panel-heading">
									<div class="panel-options">
										{% include './module_univ/programmation/inc/navigation.html.twig' %}
									</div>
								</div>
								<div class="col-md-12">
									<div class="panel-body">

										<div class="tab-content">
											<div class="tab-pane active show" id="tab-1">
												<div class="row">
													<div class="col-md-6 b-r">
														<h3 class="m-t-none m-b">Informations
														</h3>
														<div class="hr-line-dashed"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Id</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.id }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Code</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.code }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Nature épreuve</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.natureEpreuve ? UnivPrProgrammation.natureEpreuve.designation : "" }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Volume horarire</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.volumeHoraire }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Observation</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.observation }}</p>
															</div>
														</div>

														<div class="hr-line-dashed hr-line-dashed-co"></div><br/>

													</div>
													<div class="col-md-6">
														<h3 class="m-t-none m-b">Informations ACADÉMIQUE
														</h3>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Etablissement</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.element.module.semestre.promotion.formation.etablissement ? UnivPrProgrammation.element.module.semestre.promotion.formation.etablissement.designation : "" }}</p>
															</div>
														</div>

														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Formation</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.element.module.semestre.promotion.formation ? UnivPrProgrammation.element.module.semestre.promotion.formation.designation : "" }}</p>
															</div>
														</div>
														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Promotion</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.element.module.semestre.promotion ? UnivPrProgrammation.element.module.semestre.promotion.designation : "" }}</p>
															</div>
														</div>

														<div class="hr-line-dashed hr-line-dashed-co"></div>
														<div class="form-group row ph">
															<label class="col-sm-4 required font-weight-bold font-weight-bold">Année</label>
															<div class="col-sm-8">
																<p class="form-control-static">{{ UnivPrProgrammation.annee ? UnivPrProgrammation.annee.designation : "" }}</p>
															</div>
														</div>

													</div>
												</div>
											</div>
										</div>
									</div>

								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	{#{ render(controller('App\\Controller\\moduleAchat\\MaFacturefrscabController:showhistorique',{ 'facture': facture.id})) }#}
{% endblock %}
{% block javascripts %}
	{{ parent() }}{{ encore_entry_script_tags('univ_programmation') }}
{% endblock %}
