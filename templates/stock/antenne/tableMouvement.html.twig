<table class="table  tb display table-striped table-bordered table-hover dataTables-example cmd" id="gridMouvements" style="width: 100%">
    <thead>
        <tr>
            <th style="width: 10%;">#</th>
            <th style="width: 50%;">Article</th>
            <th style="width: 20%;">AjoSup</th>
            <th style="width: 30%;">Source</th>
        </tr>
    </thead>

    <tbody class="Affectation">
    {% for mouvement in mouvements %}
        <tr id="{{mouvement.id}}">
            <td class='art' >{{ loop.index }}</td>
            <td class='qte' >{{ mouvement.article.titre }}</td>
            <td class='prix' >{{ mouvement.ajosup }}</td>
            <td class='prix' >{% if mouvement.source %} {{ mouvement.source.designation }} {% endif %}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
